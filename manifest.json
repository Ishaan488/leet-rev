{
  "manifest_version": 3,
  "name": "Leet-Rev",
  "version": "1.0",
  "description": "Reads the code and provides short note for that solution.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
    "type": "module",
      "matches": ["<all_urls>"],
      "js": ["./bundle.js"]
    }
  ],
  "content_security_policy": {
    "script-src": ["'self'"],
    "object-src": ["'self'"]
  }
}
